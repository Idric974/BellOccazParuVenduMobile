{
  "name": "ParuVendu",
  "description": "Apprendre à créer une extention Chrome",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "webRequest", "tabs"],
  "action": {
    "default_popup": "popup.html",

    "default_icon": {
      "16": "/images/voiture_16x16.png",
      "32": "/images/voiture_32x32.png",
      "48": "/images/voiture_48x48.png",
      "128": "/images/voiture_128x128.png"
    }
  },
  "icons": {
    "16": "/images/voiture_16x16.png",
    "32": "/images/voiture_32x32.png",
    "48": "/images/voiture_48x48.png",
    "128": "/images/voiture_128x128.png"
  }
}
